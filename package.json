{
  "name": "react-sanitized",
  "version": "1.0.0",
  "description": "A React component that will sanitize user-inputted HTML code, using the popular sanitize-html package",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./lib --source-maps",
    "prepare": "rimraf lib && npm run build",
    "test": "jest",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marius-ionescu/react-sanitized"
  },
  "keywords": [
    "sanitize",
    "sanitizer",
    "react",
    "clean"
  ],
  "author": "Marius Ionescu <ionescu.m@outlook.com> (https://github.com/marius-ionescu/react-sanitized)",
  "license": "MIT",
  "homepage": "https://github.com/marius-ionescu/react-sanitized#readme",
  "dependencies": {
    "sanitize-html": "^1.20.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2"
  }
}
